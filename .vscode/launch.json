    "version": "0.2.0",
    "configurations": [
        {
            "name": "BigDataFile",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GeoCodeLocal/bin/Debug/net6.0/geocodeLocal.dll",
            "args": ["./data/bigInputFile.csv", "reset", "zkp"],
            "cwd": "${workspaceFolder}/GeoCodeLocal",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Swiss File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GeoCodeLocal/bin/Debug/net6.0/geocodeLocal.dll",
            "args": ["./data/swiss-addresses.csv", "reset", "samzurcher"],
            "cwd": "${workspaceFolder}/GeoCodeLocal",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}